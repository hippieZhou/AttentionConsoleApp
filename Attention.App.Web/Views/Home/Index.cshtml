@{
    ViewData["Title"] = "Home Page";
}
<!--https://mdbootstrap.com/components/bootstrap-jumbotron/-->
<div class="row pt-4">
    <div class="jumbotron text-center" style="height: 82vh;">
        <h4 class="card-title font-bold pb-2"><strong>@ViewData["Title"]</strong></h4>
        <div class="view overlay my-4">
            <img src="~/img/jumbotron.jpg" class="img-fluid" alt="">
            <a href="#">
                <div class="mask rgba-white-slight"></div>
            </a>
        </div>
        <h5 class="indigo-text font-bold mb-4">Photography</h5>
        <p class="card-text">
            Sed ut perspiciatis unde omnis iste natus sit voluptatem accusantium doloremque laudantium, totam rem aperiam.
        </p>
        <a class="fa-lg p-2 m-2 li-ic"><i class="fa fa-linkedin grey-text"> </i></a>
        <a class="fa-lg p-2 m-2 tw-ic"><i class="fa fa-twitter grey-text"> </i></a>
        <a class="fa-lg p-2 m-2 fb-ic"><i class="fa fa-facebook grey-text"> </i></a>
    </div>
</div>